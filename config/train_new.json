{
    "train_txt_db": "datasets/txt_db/nlvr2_train.db",
    "train_img_db": "datasets/img_db/nlvr2_train/",
    "val_txt_db": "datasets/txt_db/nlvr2_dev.db",
    "val_img_db": "datasets/img_db/nlvr2_dev/",
    "test_txt_db": "datasets/txt_db/nlvr2_test1.db",
    "test_img_db": "datasets/img_db/nlvr2_test/",
    "checkpoint": "datasets/pretrained/uniter-base.pt",
    "model_config": "config/uniter-base.json",
